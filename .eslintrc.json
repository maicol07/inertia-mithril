{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 2],
        "quotes": ["warn", "single"],
        "comma-dangle": ["warn", "always-multiline"]
    }
}
